<h1>All Wikis</h1>

<%= link_to "New Wiki", new_wiki_path(@wiki), class: 'btn btn-success' %>

<div class="row">
  <div class="col-md-8">
    <p class="lead"><% @wikis.each do |wiki| %></p>
      <div class="media">
        <div class="media-body">  
          <h4 class="media-heading"> 
            <%= link_to wiki.title, wiki %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
</div>


